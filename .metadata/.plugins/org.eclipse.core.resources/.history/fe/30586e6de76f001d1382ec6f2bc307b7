		ResultSet resultado = null;
		ArrayList<VendasCanceladasDTO> vendaCanceladasDTO = new ArrayList<VendasCanceladasDTO>();
		String query = "";
		
		stmt.executeQuery(query);
		return vendaCanceladasDTO;
	}

}
